per_frame_26=// custom beat detection code: (fps-independent; quiet songs don't freak out)
per_frame_27=decay_rate = pow(0.993, fps);  // lower # = more hasty to declare a beat
per_frame_28=min_att    = 2.5;               // lower # = quieter songs can declare beats
per_frame_29=decay_to   = 1.0;               // lower # = more hasty to declare a beat
per_frame_30=beat =           bass/max(min_att,bass_att);
per_frame_31=beat = max(beat, mid /max(min_att,mid_att ));
per_frame_32=beat = max(beat, treb/max(min_att,treb_att));
per_frame_33=beat = max( beat, (prev_beat-decay_to)*decay_rate + decay_to );
per_frame_34=beat_level     = (beat - prev_beat - 0.07)*24+1;
per_frame_35=is_beat = above(beat_level, 0.5);
per_frame_36=prev_beat = beat;
per_frame_37=  // put your beat responses HERE:
