per_frame_22=vol=(bass_att+mid_att+treb_att)*0.25;
per_frame_23=vol=vol*vol;
per_frame_24=mtime=mtime+vol*0.05*flip;
per_frame_25=
per_frame_26=q1=mtime*0.4;
per_frame_27=
per_frame_28=
per_frame_29=musictime=musictime+(mid*mid*mid*mid)*0.01*flip;
