per_frame_41=//flexi's beat detection (made for breakbeats)
per_frame_42=vol = bass*8 + mid*5 + treb*3;
per_frame_43=m = m*0.97 + vol*0.08;
per_frame_44=monitor = vol;
per_frame_45=beat = above(vol,res)*above(vol,m)*above(vol,16);
per_frame_46=diff = (1-beat)*diff + beat*(vol-res);
per_frame_47=res = beat*(vol + m*0.04) + (1-beat)*(res -  (0.1+diff*0.02)*60/fps);
per_frame_48=res = max(0,res);
per_frame_49=
per_frame_50=w = if(beat, int(rand(4096))/1024*asin(1),w); // on beat: randomize pos and angle of the kaleidoscope
per_frame_51=xx = if(beat, 0.1 + int(rand(800))/1000, xx);
per_frame_52=yy = if(beat, 0.1 + int(rand(800))/1000, yy);
per_frame_53=
per_frame_54=//geiss' custom beat detection code: (fps-independent; quiet songs don't freak out)
per_frame_55=decay_rate = pow(0.995, fps);  // lower # = more hasty to declare a beat
per_frame_56=min_att    = 2.2;               // lower # = quieter songs can declare beats
per_frame_57=decay_to   = 1;               // lower # = more hasty to declare a beat
per_frame_58=beat =           bass/max(min_att,bass_att);
per_frame_59=beat = max(beat, mid /max(min_att,mid_att ));
per_frame_60=beat = max(beat, treb/max(min_att,treb_att));
per_frame_61=beat = max( beat, (prev_beat-decay_to)*decay_rate + decay_to );
per_frame_62=beat_level     = (beat - prev_beat - 0.07)*24;
per_frame_63=is_beat = above(beat_level, 0.5);
per_frame_64=prev_beat = beat;
per_frame_65=
per_frame_66=w  = if(is_beat, w + (int(rand(256))-128)/1024, w);  // on beat: random increment of pos and angle
per_frame_67=xx = if(is_beat, xx + (int(rand(256))-128)/4096, xx);
per_frame_68=yy = if(is_beat, yy + (int(rand(256))-128)/4096, yy);
