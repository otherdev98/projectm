warp_19=`
warp_20=`ret.y = 0;
warp_21=`
warp_22=`if (burn > 0.1)
warp_23=`{
warp_24=`ret.x = fuel*q6*60;
warp_25=`ret.z -= ret.z*q6*120;
warp_26=`}
warp_27=`
