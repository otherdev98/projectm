per_frame_21=wave_r = .5+.3*sin(time)+.1*sin(time*10);
per_frame_22=wave_g = .5+.3*sin(time/1.5)+.1*sin(time*12);
per_frame_23=wave_b = .5+.3*sin(time/1.75)+.1*sin(time*14);
per_frame_24=bass_counter = equal(bass_thresh,2)*1+bass_counter;
per_frame_25=treb_counter=equal(treb_thresh,2)*1+treb_counter;
per_frame_26=mid_counter=equal(mid_thresh,2)*1+mid_counter;
per_frame_27=vol_counter=equal(vol_thresh,2)*1+vol_counter;
