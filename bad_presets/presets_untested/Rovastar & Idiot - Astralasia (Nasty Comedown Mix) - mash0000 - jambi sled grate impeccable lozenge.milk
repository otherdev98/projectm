per_frame_29=treb_cycler=treb_counter%8;
per_frame_30=mid_cycler=mid_counter%8;
per_frame_31=vol_cycler=vol_cycler%8;
per_frame_32=q1=bass_cycler;
per_frame_33=q2=treb_cycler;
per_frame_34=q3=mid_cycler;
per_frame_35=q4=bass_thresh;
per_frame_36=q5=mid_thresh;
per_frame_37=q6=treb_thresh;
per_frame_38=q7=vol_cycler;
per_frame_39=q8=vol_thresh;
per_frame_40=wave_mystery = wave_mystery+.1*sin(q8) - 0.1;
per_frame_41=mv_b = bass+bass_att-2;
per_frame_42=residual = equal(bass_thresh,2)*0.016*sin(time*7) + (1-equal(bass_thresh,2))*residual;
per_frame_43=echo_orient = int(2.5+residual*80);
per_frame_44=
per_frame_45=monitor = sx;
per_pixel_1=rad_cycle=rad*rad*x*60*rad*sin(q1);
per_pixel_2=rot=.06*sin(rad_cycle-3*q3);
per_pixel_3=zoom = 2*rot*rad + 1 - 0.01*q2*ang;
per_pixel_4=zoomexp = 1 - 0.05*log(rad);
per_pixel_5=cy = 0.2+ 0.3*rad + 0.1*q5;
per_pixel_6=sx = 0.3+0.68*rad+ 0.2*q6;
warp_1=`shader_body
warp_2=`{
warp_3=`    // sample previous frame
warp_4=`    ret = tex2D( sampler_main, uv ).xyz;
warp_5=`
warp_6=`    // take the difference between the crisp and blurred images,
warp_7=`    // then add it back into the image.  Creates spots and stripes over time,
warp_8=`    // basically by breaking up big blocks of white color.
warp_9=`    ret += (ret - GetBlur1(uv))*0.5;
warp_10=`    ret *= 0.9;
warp_11=`
warp_12=`   // add noise:
warp_13=`   float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.4 + rand_frame.xy;
warp_14=`   ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0 * 12 * saturate(treb_att-1);
warp_15=`
warp_16=`   // desaturate over time, to keep the globs white
warp_17=`   ret = lerp(ret, lum(ret), 0.08);
warp_18=`}
