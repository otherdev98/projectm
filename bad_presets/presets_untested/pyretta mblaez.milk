comp_2=`{
comp_3=`
comp_4=`uv = 0.5 + (uv-0.5)*0.98;
comp_5=`    float2 d = texsize.zw*4;
comp_6=`    float3 dx = ( GetBlur1(uv+float2(1,0)*d)-GetBlur1(uv-float2(1,0)*d) );
comp_7=`    float3 dy = ( GetBlur1(uv+float2(0,1)*d)-GetBlur1(uv-float2(0,1)*d) );
comp_8=`
comp_9=`float4 noise = tex3D(sampler_noisevol_hq, ((uv.xyy+float3(lum(dx),lum(dy),0)*0.5)*texsize.xyy*texsize_noisevol_hq.zww).xyz*float3(1,1,0)*0.05
comp_10=`                                        + time*float3(0,0,1)*0.2 );
comp_11=`uv -= float2(lum(dx),lum(dy))*0.04;
comp_12=`ret = abs(GetPixel(uv)+GetBlur1(uv)-GetBlur2(uv)-GetBlur3(uv));
comp_13=`ret *= lum(noise)*3; 
comp_14=`ret = pow(ret,0.5);
comp_15=`
comp_16=`//spiral collide comp
comp_17=`// ***** Flexis rotating caleidoscope code *****
comp_18=`float2 cntr = float2(q13,q14); float sin = q11; float cos = q12; float scale = q15;
comp_19=`float2 uv_r = (uv-cntr)*aspect.xy;
comp_20=`       uv_r = scale*float2( cos*uv_r.x - sin*uv_r.y, sin*uv_r.x + cos*uv_r.y); // rotation
comp_21=`       uv_r = cntr + uv_r*aspect.zw;
comp_22=`       uv_r = 1.0 - abs( frac( uv_r * 0.5 ) * 2.0 - 1.0 ); // mirror
comp_23=`sin = q17; cos = q18; scale = q16;
comp_24=`float2 uv_rr = (uv_r-cntr);
comp_25=`       uv_rr = scale*float2( cos*uv_rr.x - sin*uv_rr.y, sin*uv_rr.x + cos*uv_rr.y); // counter-rotation
comp_26=`       uv_rr = cntr + (uv_rr);
comp_27=`float t = q22;
comp_28=`float2 c = ((uv_rr-0.5));
comp_29=`float1 ang_hq = atan2(c.x,c.y);
comp_30=`float2 spiral = float2(ang_hq*q21*1.5,0.666*log(length(c))-ang_hq*q21+t);
comp_31=`spiral = 0.5 + (0.5 - abs( frac( spiral * 0.5 ) * 2.0 - 1.0 ))*float2(0.96,1.02); // mirror
comp_32=`ret = GetPixel(saturate(ret+spiral));
comp_33=`}
