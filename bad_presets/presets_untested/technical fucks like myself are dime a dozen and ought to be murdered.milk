comp_12=`
comp_13=`ret = lerp(ret, float3(1,0.5,0), length(float2(dx.x,dy.x))*2.4);
comp_14=`ret = lerp(ret,float3(1,0,0),z)*(0.9-2*(dx+dy).z);
comp_15=`ret = lerp(ret,float3(1,1,1),y)*(0.9-2*(dx+dy).y);
comp_16=`
comp_17=`ret *= 1 - (dx+dy).x;
