warp_1=`shader_body
warp_2=`{
warp_3=`uv = uv + float2(1,0)*texsize.zw;
warp_4=`    // sample previous frame
warp_5=`    ret.xyz = tex2D( sampler_main, uv ).xyz;
warp_6=`
warp_7=`      // take the difference between the crisp and blurred images,
warp_8=`      // then add it back into the image.  Creates spots and stripes over time.
warp_9=`      float2 b = lerp(GetBlur2(uv),GetBlur1(uv),uv_orig.x).xz;//GetBlur2(uv);
warp_10=`      ret.xz += (ret.xz - b)*0.3 - (((bass+treb+mid-0.5)*.333)*.02);
warp_11=`      ret.xz *= 0.95;
warp_12=`
warp_13=`      // add noise:
warp_14=`      float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.4 + rand_frame.xy;
warp_15=`      ret.xz += (tex2D(sampler_noise_lq, dither_uv).xz-0.5)/256.0 * 122 * saturate(treb_att-1);
warp_16=`
warp_17=`      // desaturate over time, to keep the globs white
warp_18=`      ret.x = lerp(ret.x, lum(ret.x), 0.2);
