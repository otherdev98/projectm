per_frame_1=ff = frame/100;
per_frame_2=wave_r = sin(5*ff/bass)/2+0.5;
per_frame_3=wave_g = cos(ff/mid)/2+0.5;
per_frame_4=wave_b = cos(3*ff/treb)/2+0.5;
