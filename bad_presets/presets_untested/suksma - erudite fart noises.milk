per_frame_76=ob_g=if(mid_flop,bass_flop,wave_g);
per_frame_77=ob_a=sound_check*(.03+.02*wave_r);
per_frame_78=ob_size=.05+.04*treb_residual;
per_frame_79=ib_r=if(sound_check,if(bass_flop,ob_b,ob_g),.5+.25*q5);
