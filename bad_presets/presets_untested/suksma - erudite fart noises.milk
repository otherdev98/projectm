per_frame_48=chaos=.9+.1*sin(pulse);
per_frame_49=entropy=if(bnot(entropy),2,if(equal(pulse,-20),1+int(rand(3)),entropy));
per_frame_50=bass_thresh = above(bass_att,bass_thresh)*2 + (1-above(bass_att,bass_thresh))*((bass_thresh-1.3)*chaos+1.3);
per_frame_51=bass_flop=abs(bass_flop-equal(bass_thresh,2));
per_frame_52=treb_thresh=above(treb_att,treb_thresh)*2 + (1-above(treb_att,treb_thresh))*((treb_thresh-1.3)*chaos+1.3);
per_frame_53=treb_flop=abs(treb_flop-equal(treb_thresh,2));
per_frame_54=mid_thresh=above(mid_att,mid_thresh)*2 + (1-above(mid_att,mid_thresh))*((mid_thresh-1.3)*chaos+1.3);
per_frame_55=mid_flop=abs(mid_flop-equal(mid_thresh,2));
per_frame_56=bass_changed=bnot(equal(old_bass_flop,bass_flop));
per_frame_57=mid_changed=bnot(equal(old_mid_flop,mid_flop));
per_frame_58=treb_changed=bnot(equal(old_treb_flop,treb_flop));
per_frame_59=bass_residual = bass_changed*sin(pulse*bass_thresh*sin(beat)) + bnot(bass_changed)*bass_residual;
per_frame_60=treb_residual = treb_changed*sin(pulse*treb_thresh*sin(beat)) + bnot(treb_changed)*treb_residual;
per_frame_61=mid_residual = mid_changed*sin(pulse*mid_thresh*sin(beat)) + bnot(mid_changed)*mid_residual;
per_frame_62=pulse=if(above(abs(pulse),20),-20,pulse+(mid_thresh+bass_thresh+treb_thresh)*.03);
per_frame_63=beat=if(above(abs(beat),20),-20,beat+.1*bor(bor(bass_changed,mid_changed),treb_changed));
per_frame_64=sound_check=above(bass_residual+treb_residual,.25*-entropy);
per_frame_65=q1=mid_residual+sin(beat)*sound_check;
per_frame_66=q2=bass_residual+sin(beat-pulse)*sound_check;
per_frame_67=q3=treb_residual+sin(pulse)*sound_check;
per_frame_68=q4=sin(pulse)+bnot(sound_check)*beat;
per_frame_69=q5=sin(beat)+bnot(sound_check)*pulse;
per_frame_70=wave_r=if(sound_check,wave_r+.5*bass_residual,1);
