warp_1=`shader_body
warp_2=`{
warp_3=`            float3 txr = float3(uv,q2);
warp_4=`
warp_5=`    float final_noise = 0;
warp_6=`    final_noise += (1.0/pow(2,0))*(tex3D(sampler_noisevol_lq, txr*pow(2,0))*2-1); 
warp_7=`    final_noise += (1.0/pow(2,1))*(tex3D(sampler_noisevol_lq, txr*pow(2,1))*2-1); 
warp_8=`    final_noise += (1.0/pow(2,2))*(tex3D(sampler_noisevol_lq, txr*pow(2,2))*2-1); 
warp_9=`    final_noise += (1.0/pow(2,3))*(tex3D(sampler_noisevol_lq, txr*pow(2,3))*2-1); 
warp_10=`
warp_11=`    final_noise =  (final_noise+1)*0.5;
warp_12=`
warp_13=`    // sample previous frame
warp_14=`    float2x2 rot =  {  q4, q5,
warp_15=`                       -q5, q4};
warp_16=`
warp_17=`
warp_18=`    ret = tex2D( sampler_main, mul(uv-0.5,rot)*(1-rad*0.05) + 0.5 + final_noise*0.01*rad ).xyz; 
warp_19=`   
warp_20=`    // darken (decay) over time
warp_21=`    ret -= 0.0; //or try: ret -= 0.004;
warp_22=`}
