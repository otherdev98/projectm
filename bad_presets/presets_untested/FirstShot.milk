per_frame_1=// custom beat detection code: (fps-independent; quiet songs don't freak out)
per_frame_2=decay_rate = pow(.993, fps);	// lower # = more hasty to declare a beat
per_frame_3=min_att    = 2.5;             // lower # = quieter songs can declare beats
per_frame_4=decay_to   = 1.0;             // lower # = more hasty to declare a beat
per_frame_5=beat =           bass/max(min_att,bass_att);
per_frame_6=beat = max(beat, mid /max(min_att,mid_att ));
per_frame_7=beat = max(beat, treb/max(min_att,treb_att));
per_frame_8=beat = max( beat, (prev_beat-decay_to)*decay_rate + decay_to );
per_frame_9=beat_level     = (beat - prev_beat - 0.07)*24;
per_frame_10=is_beat = above(beat_level, 0.5);
per_frame_11=prev_beat = beat;
per_frame_12=q1=beat_level;
per_frame_13=q2=is_beat;
per_pixel_1=dx=.0032*(2-q1)*q1*cos(rad+time*ang);	//squiggles
per_pixel_2=dy=.0032*(2-q1)*q1*sin(rad+time*ang);
per_pixel_3=//cx=sin(rad*ang*q2)/1000;
per_pixel_4=//cy=cos(rad*ang*q2)/1000;
