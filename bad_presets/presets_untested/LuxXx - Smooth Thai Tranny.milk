per_frame_17=mode = if(beat*equal(beatcounter%2,0),1-mode,mode);
per_frame_18=mode2 = if(beat,(mode2 + rand(7) + 1)%8,mode2);
per_frame_19=mode3 = if(beat,(mode3 + rand(7) + 1)%8,mode3);
