per_frame_40=avgdb=avgdb*.99+abs(db)*.01;
per_frame_41=
per_frame_42=//beatdection
per_frame_43=rawbeatb=above(db,avgdb);
per_frame_44=beatb=rawbeatb*above(time-lbbtime,.5*avgbeatrate);
per_frame_45=
per_frame_46=//beatrate calc
per_frame_47=bbtime=time*beatb;
per_frame_48=beatrate=beatb*(bbtime-lbbtime)+(1-beatb)*beatrate;
per_frame_49=avgbeatrate=beatb*(avgbeatrate*.999+beatrate*.001)+(1-beatb)*avgbeatrate;
per_frame_50=lbbtime=time*beatb+(1-beatb)*lbbtime;
per_frame_51=
per_frame_52=bc=bc+beatb;
per_frame_53=
per_frame_54=//to reset or not to reset, thats the question
per_frame_55=//maxdb=max(maxdb*.9999,abs(db));
per_frame_56=rs=bor(above(abs(db),avgdb*100),below(abs(db),avgdb*.01))*rawbeatb;
per_frame_57=drs=1-rs;
per_frame_58=//reset
per_frame_59=avgbeatrate=beatrate*rs+avgbeatrate*drs;
per_frame_60=beatrate=4*rs+beatrate*drs;
per_frame_61=bbtime=1*rs+bbtime*drs;
per_frame_62=lbbtime=1*rs+lbbtime*drs;
per_frame_63=avgdb=db*rs+avgdb*drs;
per_frame_64=bc=bc*drs+(bc-1)*rs;
