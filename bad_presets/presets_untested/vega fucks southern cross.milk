per_frame_12=mm = mm*0.99 + mid*0.02;
per_frame_13=tt = tt*0.99 + treb*0.02;
per_frame_14=
per_frame_15=mx = max(max(bb,mm),tt);
per_frame_16=mn = min(min(bb,mm),tt);
per_frame_17=
per_frame_18=t1 = (bb-mn)/(mx-mn);
per_frame_19=t2 = (mm-mn)/(mx-mn);
per_frame_20=t3 = (tt-mn)/(mx-mn);
per_frame_21=
per_frame_22=v = 0.1/fps;
per_frame_23=bm = bm + (t1-t2)*v;
per_frame_24=mt = mt + (t2-t3)*v;
per_frame_25=
per_frame_26=// coordinates spiral 1
