comp_14=`while (n <= anz) {
comp_15=`    uv2=uv;
comp_16=`    if      (n%3==0) dist = 1-frac(1.0/anz*n+q30);
comp_17=`    else if (n%3==1) dist = 1-frac(1.0/anz*n+q31);
