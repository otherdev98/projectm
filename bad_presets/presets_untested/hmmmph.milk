warp_49=`float2 uv_lens_half_sphere(float2 domain, float2 position, float radius, float refractivity){
warp_50=`   float2 polar = uv_polar(domain, position);
warp_51=`   float cone = saturate(1-polar.y/radius);
warp_52=`   float halfsphere = sqrt(1-pow(cone-1,2));
warp_53=`   float w = atan2(1-cone,halfsphere);
warp_54=`   float refrac_w = w-asin(sin(w)/refractivity);
warp_55=`   float refrac_d = 1-cone - halfsphere*tan(refrac_w);
warp_56=`   float2 refrac_uv =position+float2(sin(polar.x),cos(polar.x))*refrac_d*radius;
warp_57=`   bool mask =(length(domain-position)<radius);
warp_58=`   return !mask*domain + 
warp_59=`           mask*refrac_uv;
warp_60=`}
