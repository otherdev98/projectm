warp_39=`float2 uv_polar_logarithmic_inverse(float2 domain, float2 center, int fins, float log_factor, float2 coord){
warp_40=`   float2 polar = uv_polar(domain, center)*float2(M_INV_PI_2,1);
warp_41=`   return float2(polar.x*fins+coord.x, log_factor*log(1/polar.y) + coord.y);
warp_42=`}
warp_43=`
warp_44=`float2 uv_bipolar_logarithmic_inverse(float2 domain, float2 northPole, float2 southPole, int fins, float log_factor, float2 coord){
warp_45=`   float2 help_uv = uv_moebius_transformation(domain, northPole, southPole, 1);
warp_46=`   return uv_polar_logarithmic_inverse(help_uv,0.5,fins,log_factor,coord);
warp_47=`}
