comp_5=`
comp_6=`shader_body
comp_7=`{
comp_8=`
comp_9=`// bipolar fractal code
comp_10=`
comp_11=`uv = 0.5 + (uv-0.5)*4; // try apply some overall zoom in or out, works good in play with number of iterations
comp_12=`float cutoff = 0.96;
comp_13=`int n = 1; // number of radial mirrors
comp_14=`float s = 0.333; // logarithmic zoom factor
comp_15=`int depth = 4; //number of fractal iterations
comp_16=`
comp_17=`for(int i = 0; i<depth;i++){
comp_18=` c = (uv-0.5)*2*aspect.wz;
comp_19=`   numerator = (c + float2(q21,q22))*64;
comp_20=`   denominator = c + float2(q23,q24);
comp_21=`   fraction = float2( numerator.x*denominator.x + numerator.y*denominator.y,
comp_22=`                      numerator.y*denominator.x - numerator.x*denominator.y)/
comp_23=`                      (denominator.x*denominator.x + denominator.y*denominator.y);
comp_24=`   c = fraction-0.5; // kind of a Moebius transformation
comp_25=`   bipolar = float2(n*atan2(c.x,c.y)*M_INV_PI_2*2+q26,s*aspect.w*log(length(c))+q27);
comp_26=`   uv = 0.5 + (0.5 - abs( frac( bipolar * 0.5 ) * 2.0 - 1.0 ))*cutoff; // mirror
comp_27=`}
comp_28=`
comp_29=`
comp_30=`// lighting
comp_31=`
comp_32=`d = texsize.zw*4;
comp_33=`dx = ( 2*GetBlur1(uv + float2(1,0)*d) - 2*GetBlur1(uv-float2(1,0)*d) );
comp_34=`dy = ( 2*GetBlur1(uv + float2(0,1)*d) - 2*GetBlur1(uv-float2(0,1)*d) );
comp_35=`uv_light1 = float2((q2+q3)/2,1-(q7+q6)/2); // position of the light source
comp_36=`light1_dy = 1-pow(length( uv + float2(dx.y,dy.y) - uv_light1)*0.5,0.1);
comp_37=`
comp_38=`
comp_39=`// colors for the gradients
comp_40=`
comp_41=`c1 = float3(0.5,0,0.2);
comp_42=`c2 = float3(0,0,0.2);
comp_43=`c3 = float3(0.1,0.1,0.5);
comp_44=`c4 = float3(1,0.5,0);
comp_45=`
comp_46=`// composition of the gradients
comp_47=`
comp_48=`ret = lerp(lerp( c1, c2, uv.x+uv.y*0.5-0.1),
comp_49=`           lerp( c3, c4, 0.7-uv.y+uv.x*0.5),
comp_50=`           1-GetPixel(uv-float2(dx.y,dy.y)*texsize.zw*6).y); // separated by the green channel from the warp shader
