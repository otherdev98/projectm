warp_9=`float3 dx = tex3d(sampler_noisevol_hq, uv3d + float3(1,0,0)*d3d) - tex3d(sampler_noisevol_hq, uv3d - float3(1,0,0)*d3d);
warp_10=`float3 dy = tex3d(sampler_noisevol_hq, uv3d + float3(0,1,0)*d3d) - tex3d(sampler_noisevol_hq, uv3d - float3(0,1,0)*d3d);
warp_11=`float x =tex3d(sampler_noisevol_hq, uv3d);
warp_12=`float2 my_uv = uv + (-8*float2(dx.x,dy.x)*(1-x) - 8*float2(dy.x,-dx.x))*texsize.zw;
warp_13=`
warp_14=`ret.x = tex2D( sampler_main, my_uv ).x;
warp_15=`ret.x += (ret.x - GetBlur3(my_uv))*0.1;
warp_16=`ret.x *= 0.95;
warp_17=`ret.x +=  rand*0.4*(1-x*2);
warp_18=`
warp_19=`ret.y = GetPixel(uv_orig+rand*texsize.zw*0).y*0.96-0.004;
warp_20=`
warp_21=`float2 d = texsize.zw*4;
warp_22=`dx = GetBlur1(uv + float2(1,0)*d) - GetBlur1(uv - float2(1,0)*d);
warp_23=`dy = GetBlur1(uv + float2(0,1)*d) - GetBlur1(uv - float2(0,1)*d);
warp_24=`
warp_25=`float2 uv_blue =lerp(uv_orig,0.5+(my_uv-0.5)*1.02,-1);
warp_26=`uv_blue += float2(dx.z,dy.z)*texsize.zw*0 + rand*texsize.zw*4;
warp_27=`ret.z = GetPixel(uv_blue).z + ret.x*0.2;
warp_28=`ret.z += -(ret.z-GetBlur1(uv_blue).z)*0.1 - 0.03;
warp_29=`}
