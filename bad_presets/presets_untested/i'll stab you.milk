per_pixel_1=zoom=zoom+(above(y,.5)*sin(time)*.1);
per_pixel_2=zoom=zoom+(below(y,.5)*cos(time)*.1);
per_pixel_3=//flexi per vertex - circular canvas warps
per_pixel_4=du = (x*2-1) - (q4+q7+q10-1.5)*2/3;
per_pixel_5=dv = (y*2-1) + (q5+q8+q11-1.5)*2/3;
per_pixel_6=dist = sqrt(du*du+dv*dv);
per_pixel_7=ang2 = atan2(du,dv);
per_pixel_8=mult = sin(dist*0.05)*0.4*q14;
per_pixel_9=dx = dx+mult*sin(ang2*2+q13)*aspectx;
per_pixel_10=dy = dy+mult*cos(ang2*2+q13)*aspecty;
per_pixel_11=cx1 = 0.5+sin(time*0.618)*0.2;
per_pixel_12=cy1 = 0.5+cos(time*1.618)*0.2;
per_pixel_13=dir = bass;
per_pixel_14=d = sqrt((x-cx1)*(x-cx1)+(y-cy1)*(y-cy1));
per_pixel_15=x1 = if( above(d,0.3),0,  sin(y-cy1)*0.05*dir);
per_pixel_16=y1 = if( above(d,0.3),0, -sin(x-cx1)*0.05*dir);
per_pixel_17=cx1 = 0.5+sin(time*2.618)*0.3;
per_pixel_18=cy1 = 0.5+cos(time*3.14)*0.3;
per_pixel_19=dir = -mid*2;
per_pixel_20=d = sqrt((x-cx1)*(x-cx1)+(y-cy1)*(y-cy1));
per_pixel_21=x2 = if( above(d,0.2),0,  sin(y-cy1)*0.05*dir);
per_pixel_22=y2 = if( above(d,0.2),0, -sin(x-cx1)*0.05*dir);
per_pixel_23=cx1 = 0.5+sin(-time*2.618)*0.4;
per_pixel_24=cy1 = 0.5+cos(-time*1.14)*0.4;
per_pixel_25=dir = treb*3;
per_pixel_26=d = sqrt((x-cx1)*(x-cx1)+(y-cy1)*(y-cy1));
per_pixel_27=x3 = if( above(d,0.1),0,  sin(y-cy1)*0.05*dir);
per_pixel_28=y3 = if( above(d,0.1),0, -sin(x-cx1)*0.05*dir);
