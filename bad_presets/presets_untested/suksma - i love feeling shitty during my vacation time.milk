comp_9=`};
comp_10=`
comp_11=`
comp_12=`shader_body
comp_13=`{
comp_14=`        float2x2 rot = { cos(-q2) - 0.01, sin(-q2),
comp_15=`                    -sin(-q2), cos(-q2) - 0.01
comp_16=`              };
comp_17=`
comp_18=`    float final_noise = 0;
comp_19=`
comp_20=`    ret = tex2D(sampler_grad, mul(uv-0.5,rot) + 0.5);
comp_21=`    ret += tex2D(sampler_grad2, mul(uv-0.5,rot) + 0.5);
comp_22=`
comp_23=`    float2 uv_echo = (uv - 0.5)*1.000*float2(-1,-1) + 0.5;
comp_24=`    float3 output =  tex2D(sampler_main, uv).xyz;
comp_25=`    float3 blur =  tex2D(sampler_main, GetBlur1(uv)).xyz;
comp_26=`    
comp_27=`    ret *= cross(blur,output);
comp_28=`    ret = pow(ret,0.8);
comp_29=`    ret *= bass+mid+treb; //gamma
comp_30=`// ***** Flexis rotating caleidoscope code *****
comp_31=`float2 cntr = float2(q13,q14); float sin = q11; float cos = q12; float scale = q15;
comp_32=`float2 uv_r = (uv-cntr)*aspect.xy;
comp_33=`       uv_r = scale*float2( cos*uv_r.x - sin*uv_r.y, sin*uv_r.x + cos*uv_r.y); // rotation
comp_34=`       uv_r = cntr + uv_r*aspect.zw;
comp_35=`       uv_r = 1.0 - abs( frac( uv_r * 0.5 ) * 2.0 - 1.0 ); // mirror
comp_36=`sin = q17; cos = q18; scale = q16;
comp_37=`float2 uv_rr = (uv_r-cntr);
comp_38=`       uv_rr = scale*float2( cos*uv_rr.x - sin*uv_rr.y, sin*uv_rr.x + cos*uv_rr.y); // counter-rotation
comp_39=`       uv_rr = cntr + (uv_rr);
comp_40=`float t = q22;
comp_41=`float2 c = ((uv_rr-0.5));
comp_42=`float1 ang_hq = atan2(c.x,c.y);
comp_43=`float2 spiral = float2(ang_hq*q21*1.5,0.666*log(length(c))-ang_hq*q21+t);
comp_44=`spiral = 0.5 + (0.5 - abs( frac( spiral * 0.5 ) * 2.0 - 1.0 ))*float2(0.96,1.02); // mirror
comp_45=`if       (q30>2.3)              ret*= GetPixel(saturate(spiral));
comp_46=`else if ((q30<=2.3)&&(q30>1.7)) ret/= GetPixel(saturate(spiral));
comp_47=`else                            ret-= GetPixel(saturate(spiral));   
comp_48=`     
comp_49=`}
